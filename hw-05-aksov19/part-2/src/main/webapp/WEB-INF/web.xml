<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>hw05-part-2</display-name>

  <servlet>
    <servlet-name>cart-servlet</servlet-name>
    <servlet-class>hw05.ShoppingCartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cart-servlet</servlet-name>
    <url-pattern>/Shopping-Cart</url-pattern>
  </servlet-mapping>


  <listener>
    <listener-class>hw05.ServletContextSetup</listener-class>
  </listener>

  <listener>
    <listener-class>hw05.SessionContextSetup</listener-class>
  </listener>

</web-app>
